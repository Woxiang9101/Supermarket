
page {
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 200 rpx 0;
  box-sizing: border-box;
  background-color: #3366ff;
}
._img {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 50rpx;
  border-radius: 50%;
}
._p {
  font-size: 40rpx;
  margin-bottom: 50rpx;
}
._button {
  margin-top: 200rpx;
  margin-bottom: 100rpx;
}